{
	"name": "david63/prunelog",
	"type": "phpbb-extension",
	"description": "Adds a configurable option to once a day remove log file entries after n days.",
	"version": "3.1.0-RC1",
	"time": "2017-01-12",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=7.1.3",
		"phpbb/phpbb": ">=3.3.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "Prune Log",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "prune_log_version_file.json",
			"ssl": false
    	}
	}
}
