{
	"name": "david63/prunelog",
	"type": "phpbb-extension",
	"description": "Adds a configurable option to once a day remove log file entries after n days.",
	"version": "2.1.0",
	"time": "2017-01-12",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=5.4.0",
		"phpbb/phpbb": ">=3.2.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[*] Prune Log",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/prune_log_version_file.json"
    	}
	}
}
